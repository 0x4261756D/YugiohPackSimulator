<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
		x:Class="YugiohPackSimulator.MainWindow"
		Title="YugiohPackSimulator">
	<StackPanel>
		<Button Click="ToCreatePackClick">
			Create Pack
		</Button>
		<StackPanel Orientation="Horizontal">
			<TextBlock Margin="10">Image Url:</TextBlock>
			<TextBox x:Name="imageUrlBox"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<TextBlock Margin="10">Image Path:</TextBlock>
			<TextBox x:Name="imagePathBox"/>
		</StackPanel>
		<StackPanel Orientation="Horizontal">
			<TextBlock Margin="10">Database Root Path:</TextBlock>
			<TextBox x:Name="databaseRootPathBox"/>
		</StackPanel>
	</StackPanel>
</Window>
